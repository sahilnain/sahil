void loop()
{
  // check to see if it's time to change the state of the LED
  unsigned long currentMillis = millis();
  if((ledState2 == LOW) && (ledState3 == LOW) && (currentMillis - previousMillis1 >= interval))
  {
  
    previousMillis1 = currentMillis;
    if(ledState1 == LOW)
    {
      ledState1 = HIGH;
      digitalWrite(D0,ledState1);
      }
      else
      {
        ledState1 = LOW;
        digitalWrite(D0,ledState1);
      }
  }
  if((ledState1 == HIGH) && (ledState3 == LOW) && (currentMillis - previousMillis2 >= interval+500))
  {
  
    previousMillis2 = currentMillis;
    if(ledState2 == LOW)
    {
      ledState2 = HIGH;
      digitalWrite(D4,ledState2);
    }
      else
      {
        ledState2 = LOW;
        digitalWrite(D4,ledState2);
        }
    }
  if((ledState1 == HIGH) && (ledState2 == HIGH) && (currentMillis - previousMillis3 >= interval+1000))
  {
     
    previousMillis3 = currentMillis;
    if(ledState3 == LOW)
    {
      ledState3 = HIGH;
      digitalWrite(D5,ledState3);
    }
      else
      {
        ledState3 = LOW;
        digitalWrite(D5,ledState3);
        }
    }
  }
